    "@prisma/client": "^4.12.0",
    "@remix-run/node": "^1.14.3",
    "@remix-run/react": "^1.14.3",
    "@remix-run/vercel": "^1.14.3",
    "@vercel/node": "^2.6.2",
    "dotenv": "^16.0.3",
    "isbot": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-string-replace": "^1.1.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.14.3",
    "@remix-run/eslint-config": "^1.14.3",
